<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:c='urn:import:com.google.gwt.user.cellview.client'>

	<ui:with field='res' type='org.ssg.gui.client.userinfo.UserInfoMessages' />

	<g:HTMLPanel>
		<ul class="g_menu">
			<li>
				<g:Label ui:field="usernameLbl">...</g:Label>
			</li>
			<li style="border: 0">
				<g:Anchor href="{res.logoutLink}" ui:field="logoutBtn" >
					<ui:text from='{res.logoutLabel}' />
				</g:Anchor>
			</li>
		</ul>
	</g:HTMLPanel>

</ui:UiBinder>