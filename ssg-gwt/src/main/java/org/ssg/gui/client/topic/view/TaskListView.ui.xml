<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with type="org.ssg.gui.client.service.res.StudentHomeResources"
		field="res" />

	<ui:with field='msg' type='org.ssg.gui.client.service.res.SsgMessages' />

	<g:HTMLPanel>
		<g:FlowPanel styleName='{res.styles.widget_title}'>
			<g:Label>
				<ui:text from='{msg.topicViewTaskTitle}' />
			</g:Label>
		</g:FlowPanel>
		<g:FlowPanel styleName='{res.styles.clear}'></g:FlowPanel>
		<g:HorizontalPanel ui:field="topicTaskButtons"></g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 